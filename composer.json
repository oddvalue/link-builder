{
    "name": "oddvalue/link-builder",
    "type": "library",
    "description": "Consistent interface for generating URLs and links for models",
    "keywords": [
        "oddvalue",
        "link-builder"
    ],
    "homepage": "https://github.com/oddvalue/link-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Jim Hollington",
            "email": "jim@oddvalue.co.uk",
            "homepage": "https://github.com/oddvalue",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.3",
        "illuminate/contracts": ">=6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Oddvalue\\LinkBuilder\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Oddvalue\\LinkBuilder\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Oddvalue\\LinkBuilder\\LinkBuilderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
